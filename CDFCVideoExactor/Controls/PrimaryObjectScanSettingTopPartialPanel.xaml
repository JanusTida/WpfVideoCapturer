<UserControl x:Class="CDFCVideoExactor.Controls.PrimaryObjectScanSettingTopPartialPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CDFCVideoExactor.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Button DockPanel.Dock="Right" Margin="0,0,160,0" HorizontalAlignment="Right"
            Command="{Binding  OpenSeniorSettingCommand}"
            Template="{StaticResource TransparentButtonTempate}">
        <StackPanel>
            <Image Source="/CDFCVideoExactor;component/Images/PrimaryObjectScanSettingPage/btnSeniorScanSetting.png" Stretch="None"  />
            <TextBlock Text="{DynamicResource SeniorScanSetting}" HorizontalAlignment="Center" Foreground="White"/>
        </StackPanel>
    </Button>

</UserControl>
