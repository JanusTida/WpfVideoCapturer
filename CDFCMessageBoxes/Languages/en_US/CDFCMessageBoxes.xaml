<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:CDFCMessageBoxes.Languages.en_US">
    <!--多处共用-->
    <sys:String x:Key="OK">OK</sys:String>
    <sys:String x:Key="Cancel">Cancel</sys:String>
    <sys:String x:Key="Tip">Tip</sys:String>
    <sys:String x:Key="Yes">Yes</sys:String>
    <sys:String x:Key="No">No</sys:String>

    <!--创建案件窗体-->
    <sys:String x:Key="CaseName">Case Name</sys:String>
    <sys:String x:Key="CaseDate">Case Date</sys:String>
    <sys:String x:Key="CaseType">Case Type</sys:String>
    <sys:String x:Key="CaseNum">Case Number</sys:String>
    <sys:String x:Key="CasePath">Case Path</sys:String>
    <sys:String x:Key="CaseDisc">Case Discription</sys:String>
    <sys:String x:Key="CasePartsAndInfo">Partners,Department,Info</sys:String>
</ResourceDictionary>