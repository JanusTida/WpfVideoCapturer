<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:CDFCMessageBoxes.Languages.zh_CN">
    <!--多处共用-->
    <sys:String x:Key="OK">确定</sys:String>
    <sys:String x:Key="Cancel">取消</sys:String>
    <sys:String x:Key="Tip">提示</sys:String>
    <sys:String x:Key="Yes">是</sys:String>
    <sys:String x:Key="No">否</sys:String>
    
    <!--创建案件窗体-->
    <sys:String x:Key="CaseName">名称</sys:String>
    <sys:String x:Key="CaseDate">日期</sys:String>
    <sys:String x:Key="CaseType">类型</sys:String>
    <sys:String x:Key="CaseNum">编号</sys:String>
    <sys:String x:Key="CasePath">位置</sys:String>
    <sys:String x:Key="CaseDisc">描述</sys:String>
    <sys:String x:Key="CasePartsAndInfo">调查人员，机构，信息</sys:String>
</ResourceDictionary>